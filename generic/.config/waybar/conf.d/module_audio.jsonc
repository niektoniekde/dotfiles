{
  "pulseaudio#out": {
      "interval": 2,
      "format": "{volume:3}%",
      "format-muted": " -M-",
      "on-click": "pavucontrol",
      "tooltip": false
  },
  "pulseaudio#inp": {
      "interval": 2,
      "format": "{format_source}",
      "format-source": "{volume:3}%",
      "format-source-muted": " -M-",
      "on-click": "pavucontrol",
      "tooltip": false
  }
}
